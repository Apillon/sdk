{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "cli-install": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "dependsOn": [],
      "outputs": []
    },
    "publish-package": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    }
  }
}
